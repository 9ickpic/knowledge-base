views:
  - type: cards
    name: во что обязательно нужно поиграть
    filters:
      and:
        - file.tags.contains("whogame")
    order:
      - file.name
      - file.tags
      - file.mtime
    sort:
      - property: file.tags
        direction: ASC
    image: note.tags
    imageFit: ""
    imageAspectRatio: 1
    cardSize: 200
